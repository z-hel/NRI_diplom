            var urlstr = '/api/Nomenclature/' + id;
            await $.ajax({
                url: urlstr,
                type: 'GET',
                contentType: "application/json",
                success: function (nomenclature) {
                    name = nomenclature.name;
                },
                error: function (e) {
                    console.log(e.message);
                }
            });
            return name;
        }

