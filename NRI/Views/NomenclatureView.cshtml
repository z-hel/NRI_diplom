            //    alert('Запись удалена');

            //    var table = document.getElementById('nomenclatureTable');

            //    for (var i = 1; i < table.rows.length;) {
            //        table.deleteRow(i);
            //    }
            //    await GetNomenclatures();
            //    CloseModal();
            //}



        }

