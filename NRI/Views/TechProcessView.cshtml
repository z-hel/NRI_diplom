        }


        async function GetTechProcessById(id) {
            var urlstr = "api/TechProcess/" + id;
            await $.ajax({
                url: urlstr,
                type: 'GET',
                contentType: "application/json",
                success: async function (techProcess) {
                    var previousTO = document.getElementById('techProcessEdit');
                    previousTO.value = techProcess.name;

                    var previousNomenclature = document.getElementById('selectNomenclatureEdit');
                    previousNomenclature.value = techProcess.nomenclatureId;

                    var previousLaunchBatch = document.getElementById('launchBatchEdit');
                    previousLaunchBatch.value = techProcess.launchBatch;

                    var previousMinBatch = document.getElementById('minBatchEdit');
                    previousMinBatch.value = techProcess.minBatch;

                    var previousMaxBatch = document.getElementById('maxBatchEdit');
                    previousMaxBatch.value = techProcess.maxBatch;

                    resultForDelete.set('techProcessName', techProcess.name);
                    resultForDelete.set('nomenclatureId', techProcess.NomenclatureId);
                    resultForDelete.set('launchBatch', techProcess.launchBatch);
                    resultForDelete.set('minBatch', techProcess.minBatch);
                    resultForDelete.set('maxBatch', techProcess.maxBatch);
                },
                error: function (e) {
                    console.log(e.message);
                }
            });
        }

        function GetNomenclatures() {
            var select = document.getElementById('selectNomenclature');
            var selectEdit = document.getElementById('selectNomenclatureEdit');
            $.ajax({
                url: '/api/Nomenclature',
                type: 'GET',
                contentType: "application/json",
                success: function (nomenclatures) {
                    $.each(nomenclatures, function (index, nomenclature) {
                        var ooption = document.createElement('option');
                        ooption.value = nomenclature.id;
                        ooption.innerText = nomenclature.name
                        select.appendChild(ooption);

                        var ooptionEdit = document.createElement('option');
                        ooptionEdit.value = nomenclature.id;
                        ooptionEdit.innerText = nomenclature.name
                        selectEdit.appendChild(ooptionEdit);
                    })
                },
                error: function (e) {
                    console.log(e.message);
                }
            });
        }



        function AddTechProcess() {
            var techProcessName = document.getElementById("techProcess").value;

            var indexNomenclatureSelected = document.getElementById("selectNomenclature").options.selectedIndex;
            var nomenclatureId = document.getElementById("selectNomenclature").options[indexNomenclatureSelected].value;

            var launchBatch = document.getElementById("launchBatch").value;
            if (launchBatch.match(/^[-\+]?\d+/) === null) { alert('Укажите корректное числовое значение параметра "Количество в партии запуска"'); }

            var minBatch = document.getElementById("minBatch").value;
            if (minBatch.match(/^[-\+]?\d+/) === null) { alert('Укажите корректное числовое значение параметра "Минимальное количество в партии"'); }

            var maxBatch = document.getElementById("maxBatch").value;
            if (maxBatch.match(/^[-\+]?\d+/) === null) { alert('Укажите корректное числовое значение параметра "Максимальное количество в партии"'); }


            $.ajax({
                url: "api/techProcess",
                contentType: "application/json",
                method: "POST",
                data: JSON.stringify({
                    name: techProcessName,
                    nomenclatureId: nomenclatureId,
                    launchBatch: launchBatch,
                    minBatch: minBatch,
                    maxBatch: maxBatch
                }),
                success: function (techProcess) {
                    reset('techProcessForm');
                    alert('Запись успешно добавлена');
                    addRow('techProcessTable', techProcess);
                },
                error: function (e) {
                    console.log(e.message);
                }
            })
        }


        // Получение всех techProcess
        function GetTechProcesss() {
            $.ajax({
                url: '/api/TechProcess',
                type: 'GET',
                contentType: "application/json",
                success: function (techProcesss) {

                    $.each(techProcesss, function (index, techProcess) {
                        var transactionList = techProcess;

                        addRow("techProcessTable", transactionList);
                    })
                },
                error: function (e) {
                    console.log(e.message);
                }
            });
        }

        async function GetNomenclatureName(id) {
            var name;
            var urlstr = '/api/Nomenclature/' + id;
            await $.ajax({
                url: urlstr,
                type: 'GET',
                contentType: "application/json",
                success: function (nomenclature) {
                    name = nomenclature.name;
                },
                error: function (e) {
                    console.log(e.message);
                }
            });
            return name;
        }

