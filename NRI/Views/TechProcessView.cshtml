                var minBatchDEL = resultForDelete.get('minBatch');
                var maxBatchDEL = resultForDelete.get('maxBatch');

                var urlstr = "api/TechProcess/" + idDEL;
                $.ajax({
                    url: urlstr,
                    contentType: "application/json",
                    method: "DELETE",
                    data: JSON.stringify({
                        name: techProcessNameDEL,
                        nomenclatureId: nomenclatureIdDEL,
                        launchBatch: launchBatchDEL,
                        minBatch: minBatchDEL,
                        maxBatch: maxBatchDEL,
                        id: idDEL
