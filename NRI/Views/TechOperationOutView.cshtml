@using NRI.Models
@model NRI.Views.TechOperationOutViewModel
@{
    List<TechOperationOut> list = Model.OnGet();
}
<!DOCTYPE html>

<html lang="ru">
<head>
    <meta charset="utf-8" />
    <title>NRI</title>
    <link rel="stylesheet" href="css/main.css" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <style type="text/css">
        table {
            font-family: "Lucida Sans Unicode", "Lucida Grande", Sans-Serif;
            text-align: left;
            border-collapse: separate;
            border-spacing: 3px;
            background: #337ab7;
            color: #656665;
            /*color: rgb(162, 212, 255);*/
            border: 3px solid #337ab7;
            border-radius: 3px;
        }

        th {
            font-size: 18px;
            padding: 10px;
        }

        td {
            background: #ffffff;
            padding: 10px;
        }

        .table {
        }
    </style>
</head>


<body>
    <div class="table">
        <table>
            <tr>
                <th>Технологическая операция</th>
                <th>Номенклатура входного изделия</th>
                <th>Количество</th>
            </tr>
            @foreach (TechOperationOut obj in list)
            {
            <tr>
                <th>@obj.TechOperation</th>
                <th>@obj.NomenclatureOut</th>
                <th>@obj.Count</th>
            </tr>
            }
        </table>
    </div>
</body>


</html>
